## folder listing/traversal
alias l='ls'
alias ll='ls -hl'
alias la='ls -A'
alias lal="ls -Ahl"
alias cd..='cd ..';
alias ..='cd ..'
alias -g ...="../.."
alias -g ....="../../.."
alias -g .....="../../../.."

## file/folder manipulation
alias mkdir='nocorrect mkdir'
alias mv='nocorrect mv'
alias cp='nocorrect cp'
alias touch='nocorrect touch'
alias ln='nocorrect ln'
